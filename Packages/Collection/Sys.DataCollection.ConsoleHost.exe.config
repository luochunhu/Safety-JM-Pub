<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="AppName" value="Sys.DataCollection.ConsoleHost" />
    <add key="ConfigCenter" value="127.0.0.1:6379" />
    <add key="EnableConifgCenter" value="false" />    
    <!--RPC类型（1：Webapi; 2:Grpc）-->
    <add key="RpcModel" value="1"/>
    <!--Rpc远程服务器IP-->
    <add key="RpcRemoteIp" value="127.0.0.1"/>
    <!--Rpc远程服务器端口号-->
    <add key="RpcRemotePort" value="36000"/>
    <!--Rpc本地服务器IP-->
    <add key="RpcLocalIp" value="127.0.0.1"/>
    <!--Rpc本地服务器端口号-->
    <add key="RpcLocalPort" value="37000"/>
    <!--表示中心站为客户端还是服务端，=1为客户端，=0为服务端-->
    <add key="ServerOrClient" value="1"/>
    <!--表示中心站为客户端时，设备的连接端口号-->
    <add key="ClientPort" value="502"/>
    <!--本机监听网络IP-->
    <add key="NetServerIp" value="127.0.0.1"/>
    <!--本机监听网络端口号-->
    <add key="NetServerPort" value="7300"/>
    <add key="NetServerPort2" value="7301"/>
    <add key="NetServerPort3" value="7302"/>
    <add key="NetServerPort4" value="7303"/>
    <!--心跳检测时间间隔（单位：秒）-->
    <add key="HeartbeatInterval" value="10"/>
    <!--是否输出通讯源码，含回发与下发,默认为0-->
    <add key="ShowYmOut" value="1"/>
    <!--是否按照量程进行四舍五入，默认为直接保留两位小数，=1表示为直接保留2位小数，=2表示按照量程保留小数-->
    <add key="B4c5r" value="1"/>
    <!--馈电延迟判断时间-->
    <add key="FeedTimeOut" value="5"/>
    <!--馈电异常是否计算复电失败（1：表示计算复电失败，0：表示不计算复电失败）-->
    <add key="FeedComplexFailure" value="0"/>
   
    <!--HA双机热备路径 -->
    <add key="HAPath" value="HA\BackConfig.ini" />
    
    <!--是否写RPC调试日志   20170713-->
    <add key="IsWriteRpcLog" value="false" />

    <!--是否自动清除本地日志-->
    <add key="AutoClearLog4netLog" value="true"/>
    <!--本地日志输出路径-->
    <add key="Log4netFilePath" value="c:/log/Sys.DataCollection.ConsoleHost/"/>
    <!--清除多久前的日志-->
    <add key="ClearTimeLongAgo" value="60"/>

  </appSettings>
</configuration>